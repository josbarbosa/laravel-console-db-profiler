{
    "name": "josbarbosa/laravel-console-db-profiler",
    "description": "Laravel Console Database Profiler",
    "license": "MIT",
    "authors": [
        {
            "name": "josbarbosa",
            "email": "joseasbarbosa@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JosBarbosa\\ConsoleDbProfiler\\": "src/"
        }
    },
    "autoload-dev": {
      "classmap": [
          "tests/TestCase.php",
          "tests/TestCaseConsole.php",
          "tests/Commands/Command01.php",
          "tests/Commands/Command02.php",
          "tests/Database/Test.php",
          "tests/Utilities/MethodsBag.php",
          "tests/Utilities/MockOutput.php",
          "tests/Utilities/TestOutput.php"
      ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "JosBarbosa\\ConsoleDbProfiler\\ConsoleDbProfilerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.5.*",
        "illuminate/database": "5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "~3.0",
        "orchestra/database": "~3.5"
    }
}
